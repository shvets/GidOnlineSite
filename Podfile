source 'https://github.com/CocoaPods/Specs.git'
source 'https://github.com/shvets/Specs.git'

use_frameworks!

target 'GidOnlineSite_iOS' do
  platform :ios, '10.0'

  podspec :path => 'GidOnlineSite.podspec'

  target 'GidOnlineSite_iOSTests' do
    inherit! :search_paths
  end
end

target 'GidOnlineSite_tvOS' do
  platform :tvos, '10.10'

  podspec :path => 'GidOnlineSite.podspec'

  target 'GidOnlineSite_tvOSTests' do
    inherit! :search_paths
  end
end

